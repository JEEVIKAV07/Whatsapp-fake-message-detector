{
  "manifest_version": 3,
  "name": "WhatsApp Fake Message Detector",
  "version": "1.0",
  "description": "Detects fake forwarded messages on WhatsApp Web in real time",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["https://web.whatsapp.com/*"],
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/*"],
      "js": ["content.js"]
    }
  ]
}